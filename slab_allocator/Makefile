# Alternative GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=testing
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),testing)
  slaballocator_config = testing

else ifeq ($(config),debug)
  slaballocator_config = debug

else ifeq ($(config),release)
  slaballocator_config = release

else
  $(error "invalid configuration $(config)")
endif

PROJECTS := slaballocator

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

slaballocator:
ifneq (,$(slaballocator_config))
	@echo "==== Building slaballocator ($(slaballocator_config)) ===="
	@${MAKE} --no-print-directory -C . -f slaballocator.make config=$(slaballocator_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f slaballocator.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  testing"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   slaballocator"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"